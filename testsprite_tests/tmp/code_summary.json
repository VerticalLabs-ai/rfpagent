  "features": [
    {
      "name": "RFP Management",
      "description": "Comprehensive RFP discovery, storage, and management system with document parsing and analysis",
      "files": [
        "server/routes/rfps.routes.ts",
        "server/storage.ts",
        "shared/schema.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/api/rfps": {
            "get": {
              "summary": "Get all RFPs with pagination and filtering",
              "parameters": [
                {"name": "status", "in": "query", "schema": {"type": "string"}},
                {"name": "portalId", "in": "query", "schema": {"type": "string"}},
                {"name": "page", "in": "query", "schema": {"type": "string", "default": "1"}},
                {"name": "limit", "in": "query", "schema": {"type": "string", "default": "20"}}
              ],
              "responses": {
                "200": {"description": "List of RFPs with pagination"}
              }
            },
            "post": {
              "summary": "Create a new RFP",
              "requestBody": {
                "required": true,
                "content": {
                  "application/json": {
                    "schema": {"$ref": "#/components/schemas/RFP"}
                  }
                }
              },
              "responses": {
                "201": {"description": "RFP created successfully"}
              }
            }
          },
          "/api/rfps/{id}": {
            "get": {
              "summary": "Get a specific RFP by ID",
              "parameters": [
                {"name": "id", "in": "path", "required": true, "schema": {"type": "string"}}
              ],
              "responses": {
                "200": {"description": "RFP details"},
                "404": {"description": "RFP not found"}
              }
            }
          },
          "/api/rfps/{id}/documents": {
            "get": {
              "summary": "Get documents for a specific RFP",
              "responses": {
                "200": {"description": "List of RFP documents"}
              }
            }
          },
          "/api/rfps/detailed": {
            "get": {
              "summary": "Get detailed RFPs with compliance data",
              "responses": {
                "200": {"description": "Detailed RFPs with compliance information"}
              }
            }
          }
        }
      }
    },
    {
      "name": "Proposal Generation",
      "description": "AI-powered proposal generation with enhanced quality, compliance validation, and pipeline orchestration",
      "files": [
        "server/routes/proposals.routes.ts",
        "server/services/proposals/enhancedProposalService.ts",
        "server/services/orchestrators/proposalGenerationOrchestrator.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/api/proposals/rfp/{rfpId}": {
            "get": {
              "summary": "Get proposals for specific RFP",
              "parameters": [
                {"name": "rfpId", "in": "path", "required": true, "schema": {"type": "string"}}
              ],
              "responses": {
                "200": {"description": "List of proposals for the RFP"}
              }
            }
          },
          "/api/proposals/{id}": {
            "delete": {
              "summary": "Delete a specific proposal",
              "parameters": [
                {"name": "id", "in": "path", "required": true, "schema": {"type": "string"}}
              ],
              "responses": {
                "200": {"description": "Proposal deleted successfully"},
                "404": {"description": "Proposal not found"}
              }
            }
          },
          "/api/proposals/enhanced/generate": {
            "post": {
              "summary": "Enhanced AI-powered proposal generation",
              "requestBody": {
                "required": true,
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "rfpId": {"type": "string"},
                        "companyProfileId": {"type": "string"},
                        "options": {"type": "object"}
                      },
                      "required": ["rfpId"]
                    }
                  }
                }
              },
              "responses": {
                "200": {"description": "Proposal generation started with session ID"}
              }
            }
          },
          "/api/proposals/pipeline/generate": {
            "post": {
              "summary": "Pipeline-based proposal generation",
              "responses": {
                "200": {"description": "Pipeline generation started"}
              }
            }
          }
        }
      }
    },
