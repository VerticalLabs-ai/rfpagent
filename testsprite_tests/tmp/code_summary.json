{
  "tech_stack": [
    "TypeScript",
    "Node.js 20+",
    "Express 5",
    "React 19",
    "Vite 7",
    "PostgreSQL 16+",
    "Drizzle ORM",
    "TailwindCSS 4",
    "shadcn/ui",
    "OpenAI GPT-4/GPT-4o",
    "Anthropic Claude 4.5",
    "Mastra.ai (Agent Framework)",
    "Browserbase (Browser Automation)",
    "Stagehand (Portal Scraping)",
    "Winston Logger",
    "Zod (Schema Validation)",
    "WebSocket",
    "Server-Sent Events (SSE)"
  ],
  "features": [
    {
      "name": "RFP Discovery and Management",
      "description": "Discover, list, create, and manage Request for Proposal (RFP) opportunities from government procurement portals. Supports manual RFP creation from URLs and automated portal scanning.",
      "files": [
        "server/routes/rfps.routes.ts",
        "server/services/scrapers/mastraScrapingService.ts",
        "server/services/core/rfpService.ts",
        "server/storage/rfp.storage.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "RFP Discovery API", "version": "1.0.0"},
        "paths": {
          "/api/rfps": {
            "get": {
              "summary": "Get all RFPs with pagination and filtering",
              "parameters": [
                {"name": "status", "in": "query", "schema": {"type": "string"}},
                {"name": "portalId", "in": "query", "schema": {"type": "string", "format": "uuid"}},
                {"name": "page", "in": "query", "schema": {"type": "integer", "minimum": 1, "default": 1}},
                {"name": "limit", "in": "query", "schema": {"type": "integer", "minimum": 1, "maximum": 100, "default": 20}}
              ]
            }
          },
          "/api/rfps/manual": {
            "post": {
              "summary": "Create RFP manually from URL",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "required": ["url"],
                      "properties": {
                        "url": {"type": "string", "format": "uri"},
                        "userNotes": {"type": "string"}
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Proposal Generation Pipeline",
      "description": "AI-powered proposal generation with multi-section content creation (executive summary, technical approach, pricing), quality scoring (0.0-1.0), and iterative refinement using GPT-4 and Claude 4.5",
      "files": [
        "server/routes/proposals.routes.ts",
        "server/services/proposals/enhancedProposalService.ts",
        "server/services/orchestrators/proposalGenerationOrchestrator.ts",
        "server/services/agents/contentGeneratorAgent.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "Proposal Generation API", "version": "1.0.0"},
        "paths": {
          "/api/proposals/enhanced/generate": {
            "post": {
              "summary": "Generate enhanced AI proposal for an RFP",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "required": ["rfpId"],
                      "properties": {
                        "rfpId": {"type": "string", "format": "uuid"},
                        "companyProfileId": {"type": "string", "format": "uuid"},
                        "sessionId": {"type": "string"},
                        "options": {
                          "type": "object",
                          "properties": {
                            "generatePricing": {"type": "boolean", "default": true},
                            "generateCompliance": {"type": "boolean", "default": true},
                            "qualityThreshold": {"type": "number", "minimum": 0, "maximum": 1, "default": 0.8}
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Submission Management Pipeline",
      "description": "Complete submission workflow including portal authentication (with 2FA support), document upload, form filling, and submission tracking with retry logic and browser automation",
      "files": [
        "server/routes/submissions.routes.ts",
        "server/services/core/submissionService.ts",
        "server/services/workflows/submissionPipelineWorkflow.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "Submission Management API", "version": "1.0.0"},
        "paths": {
          "/api/submissions": {
            "get": {
              "summary": "List all submissions with filtering and pagination"
            },
            "post": {
              "summary": "Create a new submission",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "rfpId": {"type": "string", "format": "uuid"},
                        "proposalData": {"type": "object"},
                        "title": {"type": "string"},
                        "deadline": {"type": "string", "format": "date-time"}
                      }
                    }
                  }
                }
              }
            }
          },
          "/api/submissions/pipeline/start": {
            "post": {
              "summary": "Start submission pipeline workflow with browser automation",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "required": ["submissionId"],
                      "properties": {
                        "submissionId": {"type": "string", "format": "uuid"},
                        "portalCredentials": {
                          "type": "object",
                          "properties": {
                            "username": {"type": "string"},
                            "password": {"type": "string"},
                            "mfaMethod": {"type": "string"}
                          }
                        },
                        "browserOptions": {
                          "type": "object",
                          "properties": {
                            "headless": {"type": "boolean", "default": false},
                            "timeout": {"type": "integer", "minimum": 30000, "maximum": 600000}
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Portal Scanning and Monitoring",
      "description": "Automated scanning of government procurement portals (SAM.gov, BonfireHub, BeaconBid, Philadelphia) with scheduled monitoring using Browserbase and Stagehand browser automation",
      "files": [
        "server/routes/portals.routes.ts",
        "server/routes/scans.routes.ts",
        "server/services/scrapers/mastraScrapingService.ts",
        "server/services/agents/portalScannerAgent.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "Portal Scanning API", "version": "1.0.0"},
        "paths": {
          "/api/scans/trigger": {
            "post": {
              "summary": "Trigger a manual portal scan",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "required": ["portalId"],
                      "properties": {
                        "portalId": {"type": "string", "format": "uuid"},
                        "sessionId": {"type": "string"}
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Document Processing and Intelligence",
      "description": "AI-powered document parsing for PDF and Word files, requirement extraction from RFP documents, and compliance checklist generation",
      "files": [
        "server/routes/documents.routes.ts",
        "server/services/processing/documentParsingService.ts",
        "server/services/processing/requirementExtractionService.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "Document Processing API", "version": "1.0.0"},
        "paths": {
          "/api/documents/{rfpId}/parse": {
            "post": {
              "summary": "Parse RFP documents and extract requirements using AI",
              "parameters": [
                {"name": "rfpId", "in": "path", "required": true, "schema": {"type": "string", "format": "uuid"}}
              ]
            }
          }
        }
      }
    }
  ]
}
