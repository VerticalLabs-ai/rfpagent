# =============================================================================
# RFP Agent - Environment Variables Template
# Copy this file to .env and fill in your values
# =============================================================================

# =============================================================================
# DATABASE CONFIGURATION (Local Docker - Development)
# =============================================================================
DATABASE_URL="postgresql://your-database-user:your-database-password@localhost:5432/rfpagent"
PGDATABASE="rfpagent"
PGHOST="localhost"
PGPORT="5432"
PGUSER="your-database-user"
PGPASSWORD="your-database-password"

# Force use of local PostgreSQL driver (not Neon serverless)
USE_NEON="false"

# =============================================================================
# SESSION CONFIGURATION
# =============================================================================
SESSION_SECRET="generate-random-secret-key-here"
SESSION_MAX_AGE="86400000"  # 24 hours in milliseconds

# =============================================================================
# AI SERVICE API KEYS
# =============================================================================
# OpenAI Configuration
OPENAI_API_KEY="your-openai-api-key"
OPENAI_DEFAULT_MODEL="gpt-5"
OPENAI_FAST_MODEL="gpt-5-mini"

# Anthropic Configuration
ANTHROPIC_API_KEY="your-anthropic-api-key"
ANTHROPIC_DEFAULT_MODEL="claude-sonnet-4-5"
ANTHROPIC_FAST_MODEL="claude-haiku-4-5"

# Google AI Configuration
GOOGLE_API_KEY="your-google-api-key"
GOOGLE_DEFAULT_MODEL="gemini-2.5-pro"
GOOGLE_FAST_MODEL="gemini-2.5-flash"

# =============================================================================
# FEDERAL DATA APIS
# =============================================================================
# SAM.gov API Key - Required for federal procurement opportunities
# Get your API key from: https://open.gsa.gov/api/opportunities-api/
# Rate limits: 10 requests/second, 10,000 requests/day
SAM_GOV_API_KEY="your-sam-gov-api-key"

# ML Model Configuration
EMBEDDING_CACHE_SIZE="1000"  # Maximum number of cached embeddings (LRU eviction)

# =============================================================================
# BROWSERBASE CONFIGURATION
# =============================================================================
BROWSERBASE_API_KEY="your-browserbase-api-key"
BROWSERBASE_PROJECT_ID="your-browserbase-project-id"

# =============================================================================
# 1PASSWORD CONFIGURATION
# =============================================================================
OP_SERVICE_ACCOUNT_TOKEN="your-1password-service-account-token"

# =============================================================================
# GOOGLE CLOUD STORAGE (Optional)
# =============================================================================
# GCS_PROJECT_ID="your-project-id"
# GCS_BUCKET_NAME="your-bucket-name"
# GCS_CREDENTIALS_PATH="/path/to/credentials.json"

# =============================================================================
# SENDGRID EMAIL (Optional)
# =============================================================================
# SENDGRID_API_KEY="SG..."
# SENDGRID_FROM_EMAIL="noreply@rfpagent.app"

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
# development | production | test
NODE_ENV="development"
# error | warn | info | debug
LOG_LEVEL="info"
PORT="5001"
HOST="0.0.0.0"
AUTO_WORK_DISTRIBUTION="true"

# Company Configuration used in proposals and documents
COMPANY_NAME="iByte Enterprises LLC"  

# =============================================================================
# MONITORING & OBSERVABILITY (Production)
# =============================================================================
# Sentry Error Tracking (Server-side)
SENTRY_DSN="your-sentry-dsn"
SENTRY_ENVIRONMENT="production"

# Sentry Error Tracking (Client-side)
VITE_SENTRY_DSN="https://your-sentry-dsn@sentry.io/project-id"
VITE_SENTRY_DEBUG="false"

# Application Performance Monitoring
# APM_SERVER_URL=""
# APM_SECRET_TOKEN=""

# =============================================================================
# REDIS CONFIGURATION (Local Docker)
# =============================================================================
REDIS_URL="redis://localhost:6379"
REDIS_PASSWORD=""

# =============================================================================
# BACKUP CONFIGURATION
# =============================================================================
# BACKUP_ENCRYPTION_KEY="your-backup-encryption-key"
# GCS_BACKUP_BUCKET="rfp-agent-backups"
# BACKUP_RETENTION_DAYS="30"

# =============================================================================
# NOTIFICATION CONFIGURATION
# =============================================================================
# SLACK_WEBHOOK_URL="https://hooks.slack.com/services/..."
# PAGERDUTY_SERVICE_KEY=""

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
# Rate Limiting
RATE_LIMIT_WINDOW="900000"  # 15 minutes
RATE_LIMIT_MAX_REQUESTS="100"

# CORS Configuration
CORS_ORIGIN="http://localhost:5000,https://rfpagent.app"

# Cookie Configuration
COOKIE_SECURE="false"  # Set to true in production
COOKIE_SAME_SITE="lax"  # lax | strict | none

# =============================================================================
# FEATURE FLAGS
# =============================================================================
ENABLE_ANALYTICS="true"
ENABLE_AI_AGENTS="true"
ENABLE_PORTAL_MONITORING="true"
ENABLE_DEBUG_MODE="false"

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================
# Browser automation
PUPPETEER_EXECUTABLE_PATH=""  # Leave empty to use bundled Chromium

# Development helpers
SKIP_MIGRATIONS="false"
SEED_DATABASE="false"


# SAM.gov API Key
SAM_GOV_API_KEY="your-sam-gov-api-key"