    "adm-zip": "^0.5.16",
    "ai": "^5.0.0",
    "axios": "^1.12.2",
    "cheerio": "1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cron-parser": "^5.4.0",
    "date-fns": "^4.1.0",
    "domhandler": "^5.0.3",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "fast-xml-parser": "^5.3.0",
    "input-otp": "^1.4.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.545.0",
    "mammoth": "^1.11.0",
    "nanoid": "^5.1.6",
    "node-cron": "^4.2.1",
    "openai": "^6.3.0",
    "p-limit": "^7.1.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.2.16",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "vite": "^7.1.9",
