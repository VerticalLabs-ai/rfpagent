  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/express": "5.0.3",
    "@types/node": "24.7.2",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4.18",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "wait-on": "^9.0.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@sentry-internal/node-cpu-profiler",
      "@tailwindcss/oxide",
      "bufferutil",
      "esbuild",
      "protobufjs",
      "unrs-resolver"
    ],
    "overrides": {
      "google-logging-utils": "1.1.1",
      "zod": "3.25.67"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "zod": ">=3.25.0"
      }
    }
  },
  "lint-staged": {
    "server/**/*.{ts,js}": [
      "bash -c 'for file in \"$@\"; do if ! head -1 \"$file\" | grep -q \"@ts-nocheck\"; then eslint --fix \"$file\"; fi; done' --",
      "prettier --write"
    ],
    "shared/**/*.{ts,js}": [
      "bash -c 'for file in \"$@\"; do if ! head -1 \"$file\" | grep -q \"@ts-nocheck\"; then eslint --fix \"$file\"; fi; done' --",
      "prettier --write"
    ],
    "client/src/**/*.{ts,tsx,js,jsx}": [
      "bash -c 'for file in \"$@\"; do if ! head -1 \"$file\" | grep -q \"@ts-nocheck\"; then eslint --fix \"$file\"; fi; done' --",
      "prettier --write"
    ]
  }
}
