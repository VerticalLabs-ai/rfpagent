{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"NODE_ENV=development tsx server/index.ts\" \"wait-on http://localhost:3000 && vite\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "dev:backend": "NODE_ENV=development tsx server/index.ts",
    "dev:frontend": "vite",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --tree-shaking=true --minify --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "batch-compliance": "tsx server/scripts/batchProcessCompliance.ts",
    "test-compliance": "tsx server/scripts/testComplianceIntegration.ts",
    "test-proposal": "tsx scripts/test-proposal-generation.ts",
    "test-proposal-real": "tsx scripts/test-proposal-generation.ts --real-data",
    "test-proposal-fallback": "tsx scripts/test-proposal-generation.ts --test-fallback",
    "test-agents": "tsx scripts/test-agents-simple.ts",
    "test-proposals-api": "tsx scripts/test-proposals-api.ts",
    "test": "npx jest tests/",
    "test:watch": "npx jest tests/ --watch",
    "test:coverage": "npx jest tests/ --coverage",
    "lint": "eslint client/src server shared",
    "lint:ci": "eslint client/src server shared --max-warnings=2100",
    "lint:fix": "eslint client/src server shared --fix",
    "format": "prettier --write server/ shared/ client/src/",
    "format:check": "prettier --check server/ shared/ client/src/",
    "type-check": "tsc --noEmit -p tsconfig.typecheck.json",
    "quality": "npm run type-check && npm run lint && npm run format:check && npm run test",
    "prepare": "husky",
    "mastra:dev": "mastra dev",
    "mastra:build": "mastra build",
    "mastra:deploy": "mastra deploy"
  },
  "dependencies": {
    "@1password/sdk": "^0.3.1",
    "@ai-sdk/anthropic": "^2.0.27",
    "@ai-sdk/openai": "^2.0.50",
    "@browserbasehq/sdk": "^2.6.0",
    "@browserbasehq/stagehand": "^2.5.2",
    "@google-cloud/storage": "^7.17.2",
    "@hookform/resolvers": "^3.10.0",
    "@mastra/client-js": "^0.15.2",
    "@mastra/core": "^0.20.2",
    "@mastra/mcp": "^0.13.5",
    "@mastra/memory": "^0.15.6",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-accordion": "^1.2.12",
