  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  },
  "lint-staged": {
    "server/**/*.{ts,js}": [
      "bash -c 'for file in \"$@\"; do if ! head -1 \"$file\" | grep -q \"@ts-nocheck\"; then eslint --fix \"$file\"; fi; done' --",
      "prettier --write"
    ],
    "shared/**/*.{ts,js}": [
      "bash -c 'for file in \"$@\"; do if ! head -1 \"$file\" | grep -q \"@ts-nocheck\"; then eslint --fix \"$file\"; fi; done' --",
      "prettier --write"
    ],
    "client/src/**/*.{ts,tsx,js,jsx}": [
      "bash -c 'for file in \"$@\"; do if ! head -1 \"$file\" | grep -q \"@ts-nocheck\"; then eslint --fix \"$file\"; fi; done' --",
      "prettier --write"
    ]
  }
