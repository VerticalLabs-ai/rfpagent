    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@replit/vite-plugin-cartographer": "^0.3.2",
    "@replit/vite-plugin-runtime-error-modal": "^0.0.3",
    "@sentry/node": "^10.19.0",
