modules = ["nodejs-20", "web", "postgresql-16"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"
packages = ["chromium", "glib", "gtk3", "pango", "atk", "cairo", "cups", "dbus", "fontconfig", "freetype", "nss", "libxkbcommon", "mesa", "vulkan-loader", "alsa-lib", "nspr", "at-spi2-atk", "libdrm", "jq"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[env]
PORT = "5000"

[agent]
integrations = ["javascript_object_storage:1.0.0", "javascript_openai:1.0.0", "javascript_database:1.0.0", "javascript_sendgrid:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

