# Test Environment Variables
# These override .env values when running tests

# Local test database (can use SQLite or local PostgreSQL)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/rfpagent_test"
PGDATABASE="rfpagent_test"
PGHOST="localhost"
PGPORT="5432"
PGUSER="postgres"
PGPASSWORD="postgres"

# Or use Fly.io database via proxy (run: flyctl proxy 5432)
# DATABASE_URL="postgresql://fly-user:zGcAVWJrGYiTob18Rj3SmzHi@localhost:5432/fly-db"

NODE_ENV="test"
LOG_LEVEL="warn"

# Test API keys (if needed)
OPENAI_API_KEY="test-key-openai"
ANTHROPIC_API_KEY="test-key-anthropic"
GOOGLE_API_KEY="test-key-google"
